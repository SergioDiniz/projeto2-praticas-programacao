<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <navigation-rule>
        <from-view-id>/sis/ambiente/usuario/inicio.jsf</from-view-id>
        
        <navigation-case>
            <from-outcome>novadenuncia</from-outcome>
            <to-view-id>/index.jsf</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>configuracoes</from-outcome>
            <to-view-id>/configuracoes.jsf</to-view-id>
        </navigation-case>   
        
        <navigation-case>
            <from-outcome>denunciarealizada</from-outcome>
            <to-view-id>/sis/ambiente/usuario/denunciasrealizadas.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>    
    
    
</faces-config>
